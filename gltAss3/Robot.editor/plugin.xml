<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated Robot -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="simplerobot.presentation.SimplerobotModelWizardID"
            name="%_UI_SimplerobotModelWizard_label"
            class="simplerobot.presentation.SimplerobotModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SimplerobotModelFile.gif">
         <description>%_UI_SimplerobotModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated Robot -->
      <editor
            id="simplerobot.presentation.SimplerobotEditorID"
            name="%_UI_SimplerobotEditor_label"
            icon="icons/full/obj16/SimplerobotModelFile.gif"
            extensions="simplerobot"
            class="simplerobot.presentation.SimplerobotEditor"
            contributorClass="simplerobot.presentation.SimplerobotActionBarContributor">
      </editor>
   </extension>

</plugin>
